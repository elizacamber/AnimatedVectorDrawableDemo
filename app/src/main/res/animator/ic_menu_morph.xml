<?xml version="1.0" encoding="utf-8"?>

<!-- Both paths need to be compatibles to the transformation:
    They must have the same count of commands as well as the same count of parameters per command. -->
<objectAnimator
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:propertyName="pathData"
    android:duration="500"
    android:valueFrom="M20 30,L80 30,M20 50,L80 50,M20 70,L80 70"
    android:valueTo="M50 25,L80 50,M20 50,L80 50,M50 75,L80 50"
    android:valueType="pathType"/>